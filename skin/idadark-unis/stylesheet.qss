/* Scroll bar */

QScrollBar {
    background-color: #363636; /* transparent */
	border: none;
}
QScrollBar::handle {
    background-color: #585858;
	border: 1px solid #181818;
    border-radius: 5px;
}

QScrollBar:horizontal {
    height: 12px;
	margin: 0px 10px 0px 10px;
}
QScrollBar::handle:horizontal {
	/* background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop:0 #5c5c5c, stop:1 #515151); */
    min-width: 20px;
	margin: 1px 0px;
}
QScrollBar::handle:horizontal:hover {
    background-color: #181818;
	border: 1px solid #0aa;
}
QScrollBar::add-line:horizontal {
	background-color: #585858;
	border: 1px solid #181818;
	border-radius: 4px;
	
    width: 8px;
	height: 8px;
    subcontrol-position: right;
    subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
	background-color: #585858;
	border: 1px solid #181818;
	border-radius: 4px;
	
    width: 8px;
	height: 8px;
    subcontrol-position: left;
    subcontrol-origin: margin;
}
QScrollBar::add-line:horizontal:hover, QScrollBar::sub-line:horizontal:hover {
	background-color: #181818;
	border: 1px solid #0aa;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
    background: none;
}
QScrollBar:left-arrow:horizontal, QScrollBar::right-arrow:horizontal {
    width: 0px;
    height: 0px;
	/* border: 2px solid grey;
    width: 3px;
    height: 3px;
    background: white; */
}

QScrollBar:vertical {
    width: 12px;
	margin: 10px 0 10px 0;
}
QScrollBar::handle:vertical {
    /* background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop:0 #5c5c5c, stop:1 #515151); */
	min-height: 40px;
	margin: 0px 1px;
}
QScrollBar::handle:vertical:hover {
    background-color: #181818;
	border: 1px solid #0aa;
}
QScrollBar::add-line:vertical {
	background-color: #585858;
	border: 1px solid #181818;
	border-radius: 4px;
	
    width: 8px;
	height: 8px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}
QScrollBar::sub-line:vertical {
	background-color: #585858;
	border: 1px solid #181818;
	border-radius: 4px;
	
    width: 8px;
	height: 8px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}
QScrollBar::add-line:vertical:hover, QScrollBar::sub-line:vertical:hover {
	background-color: #181818;
	border: 1px solid #0aa;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

/* Tool bar */

QToolBar {
    border: 1px solid #181818;
	border-left-color: #777777;
	background-color: #363636; /*qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #666666, stop: 0.4 #555555, stop: 0.5 #3c3c3c, stop: 1.0 #3a3a3a);*/
    font-weight: bold;
}
/* QToolBar::handle:horizontal {
    image: url(<SKINDIR>/icons/Hmovetoolbar.png);
}
QToolBar::handle:vertical {
    image: url(<SKINDIR>/icons/Vmovetoolbar.png);
} */
QToolBar::separator:top,
QToolBar::separator:bottom,
QToolBar::separator:horizontal {
    /*image: url(<SKINDIR>/icons/Hsepartoolbar.png);*/
	width: 2px;
	margin: 2px 4px;
	background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop:0.5 #181818, stop:0.6 #777777);
}
QToolBar::separator:left,
QToolBar::separator:right,
QToolBar::separator:vertical {
    /* image: url(<SKINDIR>/icons/Vsepartoolbar.png); */
	height: 2px;
	margin: 4px 2px;
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop:0.5 #181818, stop:0.6 #777777);
}

QToolButton {
    background: none;
}
QToolButton::menu-indicator {
    image: url(<SKINDIR>/icons/down_arrow.png);
    top: -7px; left: -2px; /* shift it a bit */
}
QToolButton::menu-arrow {
    image: url(<SKINDIR>/icons/down_arrow.png);
}

/* PushButton */

QPushButton {
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #555555, stop: 0.4 #444444, stop: 0.5 #333333, stop: 1.0 #2d2d2d);
	border: 1px solid #181818;
    text-align: center;
    min-height: 20px;
    min-width: 50px;
    padding: 1px 8px 1px 8px;
    border-radius: 2px;
	outline: none;
}
QPushButton:default {
	border: 2px solid #181818;
	border-radius: 3px;
	color: #ffffff;
	padding: -1px;
}
QPushButton:focus {
	border: 1px solid #077;
	color: #ffffff;
}
QPushButton:hover {
    border: 1px solid #0aa;
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #666666, stop: 0.4 #555555, stop: 0.5 #444444, stop: 1.0 #2d2d2d);
	color: #ffffff;
}
QPushButton:pressed {
    border: 1px solid #0ee;   
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #444444, stop: 0.5 #2d2d2d, stop: 0.6 #555555, stop: 1.0 #777777);
	color: #ffffff;
}
QPushButton::menu-indicator  {
    subcontrol-origin: padding;
    subcontrol-position: bottom right;
    left: 8px;
}
QPushButton:disabled {
	background-color: #2d2d2d;
	border: 1px solid #181818;
	color: #666;
}

/* ComboBox */

QComboBox {
	background-color: #2a2a2a;
	selection-background-color: #3daee9;
    border: 1px solid #474747;
    border-radius: 2px;
	padding: 1px 18px 1px 3px;
	margin: 1px;
	min-width: 6em;
}
QComboBox > QLineEdit {
	border: none;
    min-height: default;
	min-width: 6em;
}
QComboBox > QLineEdit:hover, QComboBox > QLineEdit:focus {
	border: none;
	min-width: 6em;
}
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;

    border-left-width: 1px;
    border-left-color: #666;
    border-left-style: solid;
	border-top-right-radius: 2px;
	border-bottom-right-radius: 2px;
}
QComboBox::drop-down:hover {
    border-left-color: #0aa;
	background-color: #111111;
}
QComboBox::drop-down:open {
	background-color: #111111;
}
QComboBox:focus {
    border: 1px solid #777777;
	color: #ffffff;
	/* background-color: #181818; */
}
QComboBox:hover {
    border: 1px solid #0aa;
}
QComboBox::down-arrow {
    image: url(<SKINDIR>/icons/expand.png);
}
QComboBox::down-arrow:on {
    image: url(<SKINDIR>/icons/down_expand.png);
}
QComboBox:on {
	/*padding-top: 3px;
    padding-left: 4px;*/
}
QComboBox QAbstractItemView {
    border: 1px solid #686868;
	background-color: #363636;
	selection-background-color: #000;
    selection-color: #ffffff;
	color: #ddd;
	padding: 4px 1px 12px 4px;
	outline: none;
}
QComboBox QAbstractItemView::item {
    border: 1px solid #555555;
	color: #ffffff;
}
QComboBox QAbstractItemView::item:selected {
    background-color: #111111;
	border: 1px solid #555555;
	color: #ffffff;
}

/* Close, maximize and undock button for dock widgets */

IDADockWidget > QWidget > QAbstractButton {
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #777777, stop: 0.4 #666666, stop: 0.5 #444444, stop: 1.0 #4c4c4c);
	border: 1px solid #181818;
    border-radius: 2px;
}
IDADockWidget > QWidget > QAbstractButton:hover,
IDADockWidget > QWidget > QAbstractButton:default {
	background-color: #777;
    border: 1px solid #0aa;
}
IDADockWidget > QWidget > QAbstractButton:pressed {
	background-color: #888;
    border: 1px solid #0ee;
}

TNavBand > QPushButton, RegJumpButton {
	min-height: 0;
	min-width: 0;
	padding: 0 0 0 0;
	border: none;
}

EditContainer, ChooserContainer, QListView, QTreeView {
    border: 1px solid #666;
    border-radius: 2px;
	outline: none;
}

/* Remove border from IDC/Python switch button */
/* CLIWidget > QGroupBox > QPushButton, 
CLIWidget > QGroupBox > QPushButton:hover,
CLIWidget > QGroupBox > QPushButton:focus {
    border: none;
} */

CLIWidget > QGroupBox {
    margin-top: 0;
}

QTreeView::item, QListView::item, QTableView::item {
	border: 1px solid transparent;
    color: #ddd;
	outline: 0;
}
QTreeView::item:hover, QListView::item:hover, QTableView::item:hover {
	border: 1px solid #555555;
    background-color: #4c4c4c; /*474747*/
}
QTreeView::item:selected, QListView::item:selected, QTableView::item:selected {
	border: 1px solid #555555;
    background-color: #181818; /*474747*/
    color: #ffffff;
}

QAbstractView:hover {
    border: 1px solid #0aa;
}

QToolTip, QTipLabel {
    border: 1px solid #AA5500;
    border-radius: 3px;
    background: #111111;
    color: #ddd;
    margin: 0;
    padding: 1px;
}
