QApplication, QFrame, QDialog, QFileDialog, QMessageBox,
QListWidget, QTableWidget, QTextBrowser {
    background-color: #363636;
	color: #ddd;
}

QMainWindow::separator {
    background: transparent;
    border: 1px dashed #777777;
}
QMainWindow::separator:hover {
    background: #777777;
}

QSizeGrip {
    image: url(<SKINDIR>/icons/sizegrip.png);
    width: 12px;
    height: 12px;
}

QSlider:focus {
    border: none;
}

/* CheckBox and GroupBox */

QGroupBox {
    border: 1px solid #606060;
    border-radius: 2px;
	margin-top: 6px;
}
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
	padding: 0 2px;
}

QCheckBox {
	spacing: 5px;
    outline: none;
	color: #ddd;
    margin-bottom: 2px;
	border: none;
}
QCheckBox:focus {
	background: #2f2f2f;
    color: #ffffff;
}
QCheckBox:disabled {
    color: #666;
}
QCheckBox::indicator,
QGroupBox::indicator {
    width: 18px;
    height: 18px;
}
QGroupBox::indicator {
    margin-left: 2px;
	border: none;
}
QCheckBox::indicator:unchecked:focus,
QGroupBox::indicator:unchecked:focus,
QCheckBox::indicator:unchecked {
    image: url(<SKINDIR>/icons/checkbox_unchecked.png);
}
QCheckBox::indicator:unchecked:hover,
QCheckBox::indicator:unchecked:pressed,
QGroupBox::indicator:unchecked:hover,
QGroupBox::indicator:unchecked:pressed {
    image: url(<SKINDIR>/icons/checkbox_unchecked_focus.png);
}
QCheckBox::indicator:checked:focus,
QGroupBox::indicator:checked:focus,
QCheckBox::indicator:checked {
    image: url(<SKINDIR>/icons/checkbox_checked.png);
}
QCheckBox::indicator:checked:hover,
QCheckBox::indicator:checked:pressed,
QGroupBox::indicator:checked:hover,
QGroupBox::indicator:checked:pressed {
    image: url(<SKINDIR>/icons/checkbox_checked_focus.png);
}
QCheckBox::indicator:indeterminate:focus,
QCheckBox::indicator:indeterminate {
    image: url(<SKINDIR>/icons/checkbox_indeterminate.png);
}
QCheckBox::indicator:indeterminate:hover,
QCheckBox::indicator:indeterminate:pressed {
    image: url(<SKINDIR>/icons/checkbox_indeterminate_focus.png);
}
QCheckBox::indicator:checked:disabled,
QGroupBox::indicator:checked:disabled {
    image: url(<SKINDIR>/icons/checkbox_checked_disabled.png);
}
QCheckBox::indicator:unchecked:disabled,
QGroupBox::indicator:unchecked:disabled {
    image: url(<SKINDIR>/icons/checkbox_unchecked_disabled.png);
}

/* RadioButton */

QRadioButton, QLabel {
    border: 0px solid black;
}

QRadioButton {
    /*background: transparent;*/
	spacing: 5px;
    outline: none;
    color: #eff0f1;
    margin-bottom: 2px;
	border: none;
}
QRadioButton:focus {
	background: #2f2f2f;
    color: #ffffff;
	border: none;
}
QRadioButton:disabled {
    color: #666;
}
QRadioButton::indicator {
    width: 22px;
    height: 22px;
}
QRadioButton::indicator:unchecked:focus,
QRadioButton::indicator:unchecked {
    image: url(<SKINDIR>/icons/radio_unchecked.png);
}
QRadioButton::indicator:unchecked:hover,
QRadioButton::indicator:unchecked:pressed {
    image: url(<SKINDIR>/icons/radio_unchecked_focus.png);
}
QRadioButton::indicator:checked:focus,
QRadioButton::indicator:checked {
    image: url(<SKINDIR>/icons/radio_checked.png);
}
QRadioButton::indicator:checked:hover,
QRadioButton::indicator:checked:pressed {
    image: url(<SKINDIR>/icons/radio_checked_focus.png);
}
QRadioButton::indicator:checked:disabled {
    image: url(<SKINDIR>/icons/radio_checked_disabled.png);
}
QRadioButton::indicator:unchecked:disabled {
    image: url(<SKINDIR>/icons/radio_unchecked_disabled.png);
}

/* AbstractSpinBox */

QAbstractSpinBox {
	background-color: #2d2d2d;
	color: #ddd;
    border: 1px solid #363636;
    border-radius: 2px;
    min-width: 75px;
	outline: none;
}
QAbstractSpinBox:focus {
	border: 1px solid #077;
}
QAbstractSpinBox:hover {
	border: 1px solid #0aa;
}
QAbstractSpinBox:up-button {
    background-color: transparent;
    subcontrol-origin: border;
    subcontrol-position: center right;
}
QAbstractSpinBox:down-button {
    background-color: transparent;
    subcontrol-origin: border;
    subcontrol-position: center left;
}
QAbstractSpinBox::up-arrow,QAbstractSpinBox::up-arrow:disabled,QAbstractSpinBox::up-arrow:off {
    image: url(<SKINDIR>/icons/up_arrow_disabled.png);
    width: 10px;
    height: 10px;
}
QAbstractSpinBox::up-arrow:hover {
    image: url(<SKINDIR>/icons/up_arrow.png);
}
QAbstractSpinBox::down-arrow,QAbstractSpinBox::down-arrow:disabled,QAbstractSpinBox::down-arrow:off {
    image: url(<SKINDIR>/icons/down_arrow_disabled.png);
    width: 10px;
    height: 10px;
}
QAbstractSpinBox::down-arrow:hover {
    image: url(<SKINDIR>/icons/down_arrow.png);
}

/* TextEdit and PlainTextEdit */

QTextEdit, QPlainTextEdit {
    background-color: #2d2d2d;
	color: #ddd;
    border: 1px solid #363636;
    border-radius: 2px;
}
QTextEdit:focus, QPlainTextEdit:focus {
    border: 1px solid #077;
}
QTextEdit:hover, QPlainTextEdit:hover {
    border: 1px solid #0aa;
}

/* Menu and MenuBar */

QMenuBar {
	background-color: #2d2d2d; /*qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #666666, stop: 0.3 #444444, stop: 0.5 #2a2a2a, stop: 1.0 #444444);*/
    color: #ddd;
	border-bottom: 0px solid #181818;
}
QMenuBar::item {
	background: transparent;
}
QMenuBar::item:selected {
	background-color: #111111;
    border: 1px solid #666666;
	color: #ffffff;
}
QMenuBar::item:pressed {
    border: 1px solid #666666;
    background-color: #111111; /* #3087B2 */
    color: #ffffff;
    margin-bottom:-1px;
    padding-bottom:1px;
}

QMenu {
	background-color: #363636;
	color: #ddd;
	margin: 1px;
	border: 1px solid #686868;
}
QMenu::icon {
    border: 0px solid transparent;
    background-color: transparent;
	padding-left: 10px;
}
QMenu::item {
	padding: 3px 30px 3px 30px;
    border: 1px solid transparent; /* reserve space for selection border */
}
QMenu::item:selected {
    background-color: #111111;
	border: 1px solid #555555;
	color: #ffffff;
}
QMenu::separator {
    height: 2px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop:0 #000000, stop:1 #999999);
	margin: 2px 5px 2px 5px;
}
QMenu::item:disabled {
    color: #666;
}

/* LineEdit */

QLineEdit {
	background-color: #2a2a2a;
    border: 1px solid #474747;
    min-height: 18px;
    border-radius: 2px;
	color: #ddd;
	padding: 1px;
}
QLineEdit:disabled {
	color: #666;
}
QLineEdit:focus {
    border: 1px solid #077;
}
QLineEdit:hover {
	border: 1px solid #0aa;
}

/* TabWidget and TabBar */

QTabWidget {
    border: 1px solid transparent;
}
QTabWidget:focus {
    border: none;
}
QTabWidget::pane {
    background-color: #363636;
	border: 1px solid #606060;
    border-radius: 2px;
	margin: 1px;
	padding: 2px;
}
QTabWidget::pane:top {
    top: -1px;
}
QTabWidget::pane:right {
    right: -1px;
}
QTabWidget::pane:bottom {
    bottom: -1px;
}
QTabWidget::pane:left {
    left: -1px;
}

QTabBar {
    qproperty-drawBase: 0;
    left: 5px; /* move to the right by 5px */
    border-radius: 3px;
	color: #ddd;
}
QTabBar::tab {
	border: 1px solid #181818;
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #666666, stop: 0.4 #555555, stop: 0.5 #343434, stop: 1.0 #383838);
	padding: 2px 6px 2px 6px;
}
QTabBar::tab:top {
    border-top-left-radius: 6px;
    border-top-right-radius: 6px;
}
QTabBar::tab:right {
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
}
QTabBar::tab:bottom {
    border-bottom-left-radius: 6px;
    border-bottom-right-radius: 6px;
}
QTabBar::tab:left {
    border-top-left-radius: 6px;
    border-bottom-left-radius: 6px;
}
QTabBar::tab:selected {
	background-color: #2d2d2d; /* qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #666666, stop: 1 #777777); */
	border: 1px solid #606060;
	color: #ffffff;
}
QTabBar::tab:top:selected {
    border-bottom-color: #00aaff;
}
QTabBar::tab:right:selected {
    border-left-color: #00aaff;
}
QTabBar::tab:bottom:selected {
    border-top-color: #00aaff;
}
QTabBar::tab:left:selected {
    border-right-color: #00aaff;
}
QTabBar::tab:top:!selected {
    margin-top: 2px;
}
QTabBar::tab:right:!selected {
    margin-right: 2px;
}
QTabBar::tab:bottom:!selected {
    margin-bottom: 2px;
}
QTabBar::tab:left:!selected {
    margin-left: 2px;
}
QTabBar::tab:hover {
	background-color: #111111;
	border: 1px solid #666666;
	color: #ffffff;
}

/* Table */

QHeaderView {
    /* background-color: #2d2d2d;
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #666666, stop: 0.4 #555555, stop: 0.5 #444444, stop: 1.0 #3a3a3a); */
	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #666666, stop: 0.4 #4c4c4c, stop: 0.5 #343434, stop: 1.0 #383838);
	border: 1px solid #181818;
	text-align: center;
	color: #ddd;
	min-height: 22px;
}
QHeaderView::section {
	background-color: transparent; /*qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #666666, stop: 0.4 #4c4c4c, stop: 0.5 #343434, stop: 1.0 #383838);*/
    color: #ddd;
    
	padding-top: 3px;
	padding-left: 4px;
	
	border: 1px solid #666666;
    border-top-width: 0;
    border-left-color: #777777;
    border-right-color: #181818;
}
QHeaderView::section:hover {
    /* background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop:0 #64686b, stop:1 #494c4e); */
	background-color: #111111;
	color: #ffffff;
    border-bottom-color: #666;
}
QHeaderView::section:first {
    /* border-left-width: 0px; */
}
QHeaderView::section:last {
	border-right-width: 2px;
	border-right-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop:0.5 #181818, stop:0.6 #777777);
}

QTableView {
	border: 1px solid transparent; /*474747*/
    background-color: #2d2d2d;
}

QTableCornerButton::section {
	background: #363636; /* #222 */
	/*background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #666666, stop: 0.4 #555555, stop: 0.5 #444444, stop: 1.0 #3a3a3a);*/
	border: 1px solid transparent;
}

/* Splitter */

QSplitter::handle, QSplitterHandle {
    border: 1px dashed #777777;
}
QSplitter::handle:hover, QSplitterHandle:hover {
    background-color: #181818;
    border: 1px dashed #777777; /* #3daee9 */
}
QSplitter::handle:horizontal, QSplitterHandle:horizontal {
    width: 2px;
}
QSplitter::handle:vertical, QSplitterHandle:vertical {
    height: 2px;
}
QSplitter::handle:pressed, QSplitterHandle:pressed {
    background-color: #181818;
    border: 1px solid #181818;
}

/* StatusBar */

QStatusBar {
	background: transparent;
	/* border: 1px solid #181818; */
	border-top: 2px solid qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop:0.5 #181818, stop:0.6 #555555);
	border-right: 2px solid qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop:0.5 #555555, stop:0.6 #181818);
	border-bottom: 2px solid qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop:0.5 #555555, stop:0.6 #181818);
	border-left: 2px solid qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop:0.5 #181818, stop:0.6 #555555);
}
QStatusBar::item {
    margin: 1px 0px 2px 0px;
	border-right: 2px solid qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop:0.5 #181818, stop:0.6 #777777);
}
QStatusBar QLabel {
    padding: 1px 4px 1px 4px;
} /* */

