    alternate-background-color: #363636;
    color: #ddd;
    /* border: 1px solid 3A3939;
    border-radius: 2px; */
}

QAbstractScrollArea {
    border-radius: 2px;
    border: 1px solid #363636;
    background-color: transparent;
}

QStackedWidget {
    border: 1px solid transparent;
}

